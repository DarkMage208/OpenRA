<dllmap dll="OpenAL32.dll">
	<dllentry os="linux" dll="libopenal.so.1" />
	<dllentry os="windows" dll="OpenAL32.dll" />
	<dllentry os="osx" dll="/System/Library/Frameworks/OpenAL.framework/OpenAL" />
</dllmap>